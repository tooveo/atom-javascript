{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "atom-sdk/src"
    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "template": "templates/default",
    "recurse": true,
    "readme": "README.MD"
  }
}